{
  "name": "reserva-espacos",
  "version": "1.0.0",
  "description": "Sistema simples de reservas usando Node.js, Express, EJS e Prisma + SQLite",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "seed": "node scripts/seedAdmin.js"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "prisma": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
